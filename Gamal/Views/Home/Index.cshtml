<style>
    .my-card {
        background-color: transparent !important;
        border: none !important;
        padding: 0 !important;
        text-decoration: none;
    }

    .my-card {
        box-shadow: 0 2px 25px 0 rgba(110, 130, 208, .18);
        margin-bottom: 5px;
        text-decoration: none;
    }

    .my-card-body {
        background-color: #e8eeff;
    }

    .my-card-header {
        background-color: #15317E;
        margin-left: 10px;
        text-decoration: none;
    }

    .my-btn-link {
        width: 100%;
        height: 60px;
        color: white !important;
        font-weight: 500 !important;
        text-align: left !important;
        text-decoration: none !important;
    }

    .my-btn-link:hover {
        text-decoration: none;
        color: white;
    }

    .my-btn-link:after {
        content: '\2212';
        width: 35px;
        font-size: 25px;
        text-align: center;
        border-radius: 0px;
        left: 10px;
        top: 3px;
        position: absolute;
        background-color:#25587E;
        padding: 0px;
        color: white;
        border-right: 1px solid white;
        text-decoration: none;
    }

    .my-btn-link:after:hover {
        text-decoration: none;
    }

    .my-btn-link.collapsed {
        text-decoration: none;
        color: white;
    }

    .my-gray-btn-link.collapsed {
        text-decoration: none;
        color: black;
    }

    .my-btn-link.collapsed:after {
        content: '\002B';
        text-decoration: none;
    }

    .my-btn-link:before {
        text-decoration: none;
    }

    .my-btn-link:before {
        text-decoration: none;
    }

    .my-font-family {
        font-family: 'arimo','sans-serif';
        color: rgb(36, 28, 28);
        font-weight: 100;
    }

    .my-font-family:hover {
        font-family: 'arimo','sans-serif';
        color: rgb(36, 28, 28);
        font-weight: 100;
    }

    .my-gray-btn-link {
        width: 100%;
        height: 60px;
        color: black !important;
        font-weight: 200 !important;
        text-align: left !important;
        text-decoration: none !important;
    }

    .my-gray-btn-link:hover {
        text-decoration: none;
        color: black;
    }

    .my-gray-btn-link:after {
        content: '\2212';
        width: 35px;
        font-size: 25px;
        text-align: center;
        border-radius: 0px;
        left: 10px;
        top: 3px;
        position: absolute;
        background-color: #D3D3D3;
        padding: 0px;
        color: black;
        border-right: 1px solid black;
        text-decoration: none;
    }

    .my-gray-btn-link.collapsed:after {
        content: '\002B';
        text-decoration: none;
    }

    .my-gray-btn-link:before {
        text-decoration: none;
        border: 0px;
        color: white;
    }

    .nav-item.show .nav-link, .nav-tabs .nav-link.active {
        background: #25587E;
        color: white;
        font-family: 'arimo','sans-serif';
    }
    .nav-item {
        background: #1a3e78 !important;
        font-family: 'arimo','sans-serif' !important;
    }

    .nav-item > a {
        background: #1a3e78;
        font-family: 'arimo','sans-serif';
        color: white;
    }

    .nav-item > a:hover {
        background: #1a3e78;
        font-family: 'arimo','sans-serif';
        color: white;
    }
</style>

